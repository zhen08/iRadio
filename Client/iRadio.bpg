#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = iRadio.exe iRadio.ENU iRadio.CHS
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

iRadio.exe: iRadio.dpr
  $(DCC)

iRadio.ENU: ENU\iRadio.dpr
  $(DCC)

iRadio.CHS: CHS\iRadio.dpr
  $(DCC)


